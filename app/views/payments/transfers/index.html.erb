<p id="notice"><%= notice %></p>

<h3>Webhook Subscription</h3>

<div class="webhooks">
  <table>
    <thead>
    <tr>
      <th>API Secret Key</th>
      <th>Event Type</th>
      <th>Location Resource</th>
      <th>Access Token</th>
      <th>Response</th>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @webhooks.each do |webhook| %>
      <tr>
        <td><%= webhook.webhook_secret %></td>
        <td><%= webhook.event_type %></td>
        <td><%= webhook.location_url %></td>
        <td><%= webhook.access_token %></td>
        <td><%= webhook.response %></td>
        <td><%= link_to 'Show', webhook %></td>
        <!--      <td><%#= link_to 'Edit', edit_trial_path(trial) %></td>-->
        <!--      <td><%#= link_to 'Destroy', webhooks, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
      </tr>
    <% end %>
    </tbody>
  </table>
</div>

<br>

<%= link_to 'New Webhook Subscription', new_webhook_path, class: "waves-effect waves-light btn" %>

